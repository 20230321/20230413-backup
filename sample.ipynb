   "source": [
    "import numpy as np\n",
    "from nolds import sampen\n",
    "\n",
    "# 生成隨機數據\n",
    "data = np.random.rand(100)\n",
    "\n",
    "# 設置樣本熵參數（m = 2, r = 0.2 * std(data)）\n",
    "m = 2\n",
    "r = 0.2 * np.std(data)\n",
    "\n",
    "# 計算樣本熵\n",
    "sample_entropy = sampen(data, emb_dim=m, tolerance=r)\n",
    "\n",
    "print(\"Sample entropy:\", sample_entropy)"
   ]
